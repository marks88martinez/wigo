  <body>
    <nav class="container navbar fixed-top charming-navbar navbar-toggleable-md navbar-light bg">
          <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbar-nav" aria-controls="navbar-nav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>

          <%= link_to  root_path , {:class => "navbar-brand mr-5 logo"} do%>
            <img class="navbar-brand-logo" src="/img/logo2.png" >
          <% end%>


          <div class="navbar-collapse justify-content-md-end collapse " id="navbar-nav">
            <ul class="navbar-nav charming-mobile-nav">
              <li class="nav-item dropdown">
                <a class="nav-link section active " id="s1" href="#1">
                  HOME
                </a>

              </li>

              <li class="nav-item ">
                <a class="nav-link section " id="s2" href="#2" >
                  EMPRESA
                </a>

              </li>
              <li class="nav-item dropdown">
                <a class="nav-link section "  id="s3"href="#3">
                  QUE HACEMOS
                </a>

              </li>


              <li class="nav-item">
                <a href="#4"  id="s4" class="nav-link section">CONTACTO</a>
              </li>
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="http://example.com" id="dropdown01" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">IDIOMAS</a>
                <div class="dropdown-menu" aria-labelledby="dropdown01">
                  <% @idiomas.each do |idioma|  %>
                  <a class="dropdown-item" href="<%= idioma.link.blank? ? "#" : idioma.link %>"><%=idioma.nombre%></a>
                  <% end %>
                </div>
              </li>

            </ul>
          </div>
        </nav>


  <section id="1">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <ol class="carousel-indicators">
        <% @banner.each_with_index do |banner, index| %>
        <li data-target="#myCarousel" data-slide-to="index" class="<%= index == 0 ? 'active': ' '  %>"></li>
        <% end %>


      </ol>





      <div class="carousel-inner" role="listbox">
      <% @banner.each_with_index do |banner, index| %>
        <div class="carousel-item <%= index == 0 ? 'active': ' '  %>">
          <%= image_tag banner.picture.url(:large), class: "first-slide" %>
          <!-- <img class="first-slide" src="https://cdn-images-1.medium.com/max/2000/1*z9l86Mv1505QIgQfSqeW5A.jpeg" alt="First slide"> -->
          <div class="container">
            <% if !(banner.titulo.blank? &&  banner.descripcion.blank?) %>
            <div class="carousel-caption d-none d-md-block text-left bg2">
              <h1 class="animated <%=banner.a_titulo%>"><%= banner.titulo %></h1>
              <p class="animated <%=banner.a_descripcion%> "><%= banner.descripcion %></p>
              <!-- <p><a class="btn btn-lg btn-primary" href="https://v4-alpha.getbootstrap.com/examples/carousel/#" role="button">Sign up today</a></p> -->
            </div>
            <% end %>
          </div>
        </div>
      <% end %>
      </div>

      <!-- <a class="carousel-control-prev" href="#" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a> -->
    </div>

  </section>
<!--///////////////////////-->
<% @empresas.each do |emp| %>
