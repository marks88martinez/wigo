    <div class="row featurette wow slideInRight  ">
        <div class="col-md-7">
          <h2 class="featurette-heading "><%= emp.titulo %> <span class="text-muted"></span></h2>
          <p class="lead">
            <%= emp.descripcion %>

            </p>
        </div>
        <div class="col-md-5 wow slideInLeft">
          <%= image_tag emp.picture.url(:large), class: "featurette-image img-fluid mx-auto" %>
        </div>
      </div>
      <hr class="featurette-divider">
  </section>
  <% end %>



  <!--///////////////////////-->
    <section class="container " id="3">
      <section class="jumbotron text-center wow bounceInLeft  ">
      <div class="container ">
